# .env.example
# Copy this file to .env and fill in actual values

AUTOTOOL_VALIDATE_ON_IMPORT=false

# ============================================================================
# API Configuration
# ============================================================================
OPENAI_BASE_URL=https://api.example.com/v1
OPENAI_API_KEY=your_api_key_here

# ============================================================================
# Model Path Configuration
# ============================================================================
SIMCSE_MODEL_PATH=/path/to/simcse/model
TRANSFORMERS_CACHE_DIR=/path/to/cache

# ============================================================================
# Tool Description File Path
# ============================================================================
TOOL_DESC_FILE=/path/to/tool_description.json

# ============================================================================
# N-gram Model Configuration (Optional)
# ============================================================================
# Training log file path (leave empty to use default)
NGRAM_LOG_FILE=
# Output model file path (leave empty to use default: ./ngram_models/ngram_model.pkl)
NGRAM_MODEL_OUTPUT=
# N-gram value (2=bigram, 3=trigram, etc.)
NGRAM_N_VALUE=3
# Enable debug mode
NGRAM_DEBUG=false

# ============================================================================
# Output Paths
# ============================================================================
OUTPUT_DIR=./results
PIE_CHART_DIR=./tool_successor_pie_charts
MARKOV_ANALYSIS_DIR=./markov_analysis

# ============================================================================
# Model Inference Parameters
# ============================================================================
MODEL_NAME=qwen/qwen2.5-7b-instruct
TEMPERATURE=0
TOP_P=0.95
SEED=42
